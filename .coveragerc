[run]
source = persistent
# New in 5.0; required for the GHA coveralls submission.
relative_files = True
omit =
     persistent/_ring_build.py

[report]
exclude_lines =
    pragma: no cover
    class I[A-Z]\w+\((Interface|I[A-Z].*)\):
    raise NotImplementedError
    raise AssertionError
    if __name__ == '__main__':
